<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.java.j2seproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2se-project/3">
            <name>graphics</name>
            <explicit-platform explicit-source-supported="true"/>
            <source-roots>
                <root id="src.dir"/>
                <root id="src.resources.dir" name="Resources"/>
                <root id="src.gen-jsl-decora.dir" name="JSL Decora Generated Sources"/>
                <root id="src.res-jsl-decora.dir" name="JSL Decora Resources"/>
                <root id="src.gen-jsl-prism.dir" name="JSL Prism Generated Sources"/>
                <root id="src.res-jsl-prism.dir" name="JSL Prism Resources"/>
            </source-roots>
            <test-roots>
                <root id="test.java.dir" name="Test Sources"/>
                <root id="test.resources.dir" name="Test Resources"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>base</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>buildSrc</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
</project>
